<%}%>



<script>

  var userId

  var popup
  var currentUserEmail
  var userIdB
  var orderId
  var selectedUser
  var CATEGORY
  var domain_without_protocol
  var user
  var orderId
  var bidblock_email




  switch (URL_) {
    case '/operations/articles-dashboard':
    case `/operations/monitor-messages/${caseUserID}/edit-see`:
    case '/users/login':
    case '/btclayerexchange':
    // case `/make/${caseOptionMakes}`:
      break;

    case '/subscription':
      bidblock_email = '<%-locals.ENV.bidblock_email%>'
      console.log("------------->>>>", bidblock_email)
      break

    case '/':
      userId = '<%= locals.userId %>'
      popup = '<%= locals.popup %>'

      // if(`< %=locals.selectedUser%>`){
      //   selectedUser = < %-JSON.stringify(locals.selectedUser || {})%> 
      // }
      // selectedUser
      console.log("------------->>>>>>", userId)
      break;



    case '/users/profile':
      userId = '<%= locals.userId %>'
      if(`<%=locals.selectedUser%>`){
        selectedUser = <%-JSON.stringify(locals.selectedUser || {})%> 
      }
      console.log("------------->>>>>>", userId, selectedUser)
      break;


    case `/btclayerexchange/${caseOptionBTClayerexchange}`:
      userId = '<%= locals.userId %>'
      console.log("------------->>>>>>", userId)
      break;

      
      

    case `/marketplace/${caseOptionMarketplace}`:
      userId = '<%= locals.userId %>'
      popup = '<%= locals.popup %>'
      console.log("------------->>>>>>", userId, popup)
      break;
    case `/marketplace/${caseOptionMarketplace}/${caseOptionMarketplace2}`:
      userId = '<%= locals.userId %>'
      // orderId = '< %= locals.userId %>'
      console.log("------------->>>>>>", userId)
      break;





    case '/articles':
    case `/articles/${caseCategory}`:
    case `/articles/individual_article/${caseArticleID}`:
      CATEGORY = <%-JSON.stringify(locals.CATEGORY || {})%> 
      console.log(CATEGORY)
      break;
    



    case '/messaging':
      userId = '<%= locals.userId %>'
      currentUserEmail = '<%= locals.currentUserEmail  %>'
      user = <%-JSON.stringify(locals.user || {})%> 
      domain_without_protocol = '<%-locals.ENV.domain_without_protocol%>'


      userIdB = '<%= locals.userIdB  %>'
      orderId = '<%= locals.orderId  %>'

      console.log("--------------->>>>>>", userId, currentUserEmail, userIdB, orderId, user, domain_without_protocol)

      break;
    case '/messaging/messages':
      userId = '<%= locals.userId %>'
      currentUserEmail = '<%= locals.currentUserEmail  %>'
      user = <%-JSON.stringify(locals.user || {})%> 

      console.log("--------------->>>>>>", userId, currentUserEmail, user)
      break;





    case '/operations':
      // CATEGORY = < %-JSON.stringify(locals.CATEGORY || {})%> 

      user = <%-JSON.stringify(locals.user || {})%> 
      console.log("--------------->>>>>>", user, CATEGORY)
      break;
    case `/operations/${caseOperationsTask}`:
      if (caseOperationsTask == "create-article") {
        CATEGORY = <%-JSON.stringify(locals.CATEGORY || {})%> 
        console.log(CATEGORY)
      }
      if (caseOperationsTask == "monitor-messages") {
        user = <%-JSON.stringify(locals.user || {})%> 
        console.log("--------->>>>", user)
      }
      break;
    case `/operations/${caseOperationsTask}/${caseUserID}`:
      if (caseOperationsTask == "monitor-messages") {
        user = <%-JSON.stringify(locals.user || {})%> 
        console.log("--------------->>>>>>", user)
      }
      break;
    default:
      break;
  }
</script>