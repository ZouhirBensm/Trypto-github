<%- include("declarePathParams") %>

<%if(locals.paths_URL!=null && locals.paths_URL[0] == 'articles'){%>
  <%- include("setupPathParams4articles") %>
<%}%>
<%if(locals.paths_URL!=null && locals.paths_URL[0] == 'operations'){%>
  <%- include("setupPathParams4operations") %>
<%}%>
<%if(locals.paths_URL!=null && locals.paths_URL[0] == 'databases'){%>
  <%- include("setupPathParams4databases") %>
<%}%>
<%if(locals.paths_URL!=null && locals.paths_URL[0] == 'make'){%>
  <%- include("setupPathParams4make") %>
<%}%>



<script>


  let OBJLOCALS = {}

  var environment
  var navBars
  var loggedIn
  var isPaypalScriptNeeded
  var paypal_client_id
  var userId
  var popup
  var currentUserEmail
  var userIdB
  var orderId
  var selectedUser2
  var CATEGORY2



  environment = '<%= locals.ENV.environment %>'
  navBars = '<%= navBars %>'
  loggedIn = '<%= loggedIn %>'


  switch (URL_) {
    case '/operations':
    case '/operations/articles-dashboard':
    case `/operations/monitor-messages/${caseUserID}/edit-see`:
    case `/articles/individual_article/${caseArticleID}`:
    case `/articles/${caseCategory}`:
    case '/users/login':
    case '/databases':
    case `/make/${caseOptionMakes}`:
      break;

    case '/':
      userId = '<%= locals.userId %>'
      popup = '<%= locals.popup %>'

      if(`<%=locals.selectedUser%>`){
        selectedUser2 = <%-JSON.stringify(locals.selectedUser || {})%> 
      }

      console.log(userId, popup, selectedUser2)

      break;
    case '/subscription':
      isPaypalScriptNeeded = <%= locals.isPaypalScriptNeeded %>
      paypal_client_id = '<%= locals.ENV.paypal_client_id %>'

      console.log(isPaypalScriptNeeded, paypal_client_id)
      break;
    case '/users/profile':
      userId = '<%= locals.userId %>'

      if(`<%=locals.selectedUser%>`){
        selectedUser2 = <%-JSON.stringify(locals.selectedUser || {})%> 
      }

      console.log(selectedUser2, userId)
      
      break;
    case `/databases/${caseOptionDatabases}`:
      userId = '<%= locals.userId %>'

      console.log(userId)
      break;
    case '/articles':
      CATEGORY2 = <%-JSON.stringify(locals.CATEGORY || {})%> 

      console.log(CATEGORY2)
      break;
    
    case '/messaging':
      userId = '<%= locals.userId %>'
      currentUserEmail = '<%= locals.currentUserEmail  %>'
      userIdB = '<%= locals.userIdB  %>'
      orderId = '<%= locals.orderId  %>'
      user = <%-JSON.stringify(locals.user || {})%> 
      
      console.log(userId, currentUserEmail, userIdB, orderId, user)
      break;
    case '/messaging/messages':
      userId = '<%= locals.userId %>'
      currentUserEmail = '<%= locals.currentUserEmail  %>'
      user = <%-JSON.stringify(locals.user || {})%> 
      
      console.log(userId, currentUserEmail, user)
      break;
    case `/operations/${caseOperationsTask}`:
      if (caseOperationsTask == "create-article") {
        CATEGORY2 = <%-JSON.stringify(locals.CATEGORY || {})%> 
      }

      console.log(CATEGORY2)
      break;

    case `/operations/${caseOperationsTask}/${caseUserID}`:
      if (caseOperationsTask == "monitor-messages") {
        user = <%-JSON.stringify(locals.user || {})%> 
      }

      console.log(user)
      break;

    default:
      break;
  }
</script>